    <execute>avt.RedirectToolkit.Action.ClearCookies,avt.RedirectToolkit</execute>
    <settings-ctrl>ClearCookiesSettings.ascx</settings-ctrl>
    <validate-fn>validateClearCookiesSettings</validate-fn>
</action>